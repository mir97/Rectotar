        </dt>
        <dd>
            @Html.DisplayFor(model => model.Indicator.IndicatorUnit)
        </dd>
        <dt>
            Тип показателя
        </dt>
        <dd>
            @Html.DisplayFor(model => model.Indicator.IndicatorType)
        </dd>
    </dl>

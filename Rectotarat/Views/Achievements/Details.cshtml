    <hr />
    <dl class="dl-horizontal">
        <dt>
            Название показателя
        </dt>
        <dd>
            @Html.DisplayFor(model => model.Indicator.IndicatorName)
        </dd>
        <dt>
            Год
        </dt>
        <dd>
            @Html.DisplayFor(model => model.Year)
        </dd>
        <dt>
            Описани
        </dt>
        <dd>
            @Html.DisplayFor(model => model.Indicator.IndicatorDescription)
        </dd>
        <dt>
